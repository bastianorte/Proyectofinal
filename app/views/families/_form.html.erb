  <%= form_with(model: family, local: true) do |form| %>


    <div class="field">
      <%= form.label :name %>
      <%= form.text_field :name %>
    </div>

    <div class="field">
      <%= form.label :photo %>
      <%= form.file_field :photo %>
    </div>

    <div class="actions">
      <%= form.submit "Enviar", class: "btn btn-primary" %>
    </div>
  <% end %>
