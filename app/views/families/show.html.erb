<div class="container pt-4">
<%= form_with(model: @family, local: true) do |form| %>


  <div class="field">
    <%= form.label :name %>
    <%= form.text_field :name %>
  </div>

  <div class="field">
    <%= form.label :photo %>
    <%= form.text_field :photo %>
  </div>

  <div class="actions">
    <%= form.submit "Actualizar", class: "btn btn-primary" %>
  </div>
<% end %>
</div>
